{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "pnpm": {
    "executionEnv": {
      "nodeVersion": "22.14.0"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "msw",
      "unrs-resolver"
    ]
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky"
  },
  "dependencies": {
    "@nuxt/eslint": "1.3.0",
    "@nuxt/fonts": "0.11.2",
    "@nuxt/test-utils": "3.17.2",
    "@nuxtjs/fontaine": "0.5.0",
    "@pinia/nuxt": "^0.11.0",
    "@provetcloud/css": "^1.1.0",
    "@provetcloud/themes": "^1.0.0",
    "@provetcloud/web-components": "^1.3.3",
    "@vueuse/nuxt": "13.1.0",
    "eslint": "^9.0.0",
    "nuxt": "^3.17.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1",
    "zod": "^3.24.4"
  },
  "packageManager": "pnpm@10.10.0",
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@provetcloud/web-components-vue-types": "^1.3.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "msw": "^2.7.5",
    "typescript": "^5.8.3",
    "vite-plugin-checker": "^0.9.1",
    "vite-plugin-eslint2": "^5.0.3",
    "vitest": "^3.1.2"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "pnpm lint:fix"
    ]
  },
  "msw": {
    "workerDirectory": [
      ""
    ]
  }
}
